{
  "extends": [
    "config:base"
  ],
  "timezone": "Asia/Tokyo",
  "repositories": [
    "yukihir0/yubi"
  ],
  "baseBranches": [
    "main"
  ],
  "enabledManagers": [
    "cargo",
    "dockerfile"
  ],
  "rangeStrategy": "pin",
  "labels": [
    "renovate"
  ],
  "packageRules": [
    {
      "groupName": "prost",
      "matchPackageNames": [
        "prost",
        "prost-types"
      ]
    },
    {
      "groupName": "serde",
      "matchPackageNames": [
        "serde",
        "serde_yaml"
      ]
    }
  ]
}